<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>MyWatchWatch</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="robots" content="all,follow">
    <!-- Bootstrap CSS-->
    <link href="~/Library/Admin/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Font Awesome CSS-->
    <link rel="stylesheet" href="~/Library/Admin/vendor/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.1/css/all.css" integrity="sha384-5sAR7xN1Nv6T6+dT2mhtzEpVJvfS3NScPQTrOxhwjIuvcA67KV2R5Jz6kr4abQsz" crossorigin="anonymous">
    <!-- Fontastic Custom icon font-->
    <link href="~/Library/Admin/css/fontastic.css" rel="stylesheet" />

    <link href="~/Library/Admin/css/grasp_mobile_progress_circle-1.0.0.min.css" rel="stylesheet" />
    <!-- Google fonts - Roboto -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700">
    <!-- jQuery Circle-->
    <!-- Custom Scrollbar-->
    <link rel="stylesheet" href="~/Library/Admin/vendor/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.css">
    <!-- theme stylesheet-->
    <link href="~/Library/Admin/css/style.default.css" rel="stylesheet" />

    <!-- Custom stylesheet - for your changes-->
    <link href="~/Library/Admin/css/custom.css" rel="stylesheet" />

    <!-- Favicon-->

    <link rel="shortcut icon" href="~/Library/myIcon.ico">
    <!-- Tweaks for older IEs-->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
</head>
<body>
    <!-- Side Navbar -->
    <nav class="side-navbar">
        <div class="side-navbar-wrapper">
            <!-- Sidebar Header    -->
            <div class="sidenav-header d-flex align-items-center justify-content-center">
                <!-- User Info-->
                <div class="sidenav-header-inner text-center">

                    <img src="~/Library/Admin/img/avatar-2.jpg" alt="person" class="img-fluid rounded-circle">
                    <h2 class="h5">Teams 03</h2><span>Web Developer</span>
                </div>
                <!-- Small Brand information, appears on minimized sidebar-->
                <div class="sidenav-header-logo"><a href="@Url.Action("Index","Page")" class="brand-small text-center"> <strong>B</strong><strong class="text-primary">D</strong></a></div>
            </div>
            <!-- Sidebar Navigation Menus-->
            <div class="main-menu">
                <h5 class="sidenav-heading">Main</h5>
                <ul id="side-main-menu" class="side-menu list-unstyled">
                    <li><a href="@Url.Action("Index","Page")"> <i class="icon-home"></i>Home</a></li>
                    <li><a href="#"> <i class="fa fa-user"></i>Contacts</a></li>


                    <li>
                        <a href="#exampledropdownDropdown" aria-expanded="false" data-toggle="collapse"> <i class="fa fa-balance-scale"></i>My Watch </a>
                        <ul id="exampledropdownDropdown" class="collapse list-unstyled ">
                            <li><a href="@Url.Action("Index","Categories")">Category</a></li>
                            <li><a href="@Url.Action("Index","Suppliers")">Supplier</a></li>
                            <li><a href="@Url.Action("Index","Products")">Watch</a></li>
                            <li><a href="@Url.Action("Index","Promotions")">Promotion</a></li>
                        </ul>
                    </li>
                    <li><a href="#"> <i class="fa fa-bar-chart"></i>Statistical <div class="badge badge-warning">6 </div></a></li>

                </ul>
            </div>
            <div class="admin-menu">
                <h5 class="sidenav-heading">My Company</h5>
                <ul id="side-admin-menu" class="side-menu list-unstyled">
                    <li> <a href="@Url.Action("Index","Employees")"> <i class="fa fa-address-book"> </i>Employee</a></li>
                    <li>
                        <a href="#">
                            <i class="fa fa-cog"> </i>About US
                            <div class="badge badge-info">Special</div>
                        </a>
                    </li>
                    <li> <a href="@Url.Action("Index","Deliveries")"> <i class="fa fa-car"> </i>Delivery</a></li>
                    <li> <a href="@Url.Action("Index","Branches")"> <i class="fa fa-external-link"> </i>Branchs</a></li>
                    <li> <a href="@Url.Action("Index","News")"> <i class="fa fa-users"> </i>News</a></li>
                    <li> <a href="@Url.Action("Index","Orders")"> <i class="fa fa-cart-plus"> </i>Information Order</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="page">
        <!-- navbar-->
        <header class="header">
            <nav class="navbar">
                <div class="container-fluid">
                    <div class="navbar-holder d-flex align-items-center justify-content-between">
                        <div class="navbar-header">
                            <a id="toggle-btn" href="#" class="menu-btn"><i class="icon-bars"> </i></a><a href="@Url.Action("Index","Page")" class="navbar-brand">
                                <div class="brand-text d-none d-md-inline-block"><strong class="text-primary">Dashboard</strong></div>
                            </a>
                        </div>
                        <ul class="nav-menu list-unstyled d-flex flex-md-row align-items-md-center">
                            <li class="nav-item"><a href="#" class="nav-link logout"> <span class="d-none d-sm-inline-block">Hi @Session["employee"]</span><i class="fa fa-cog"></i></a></li>
                            <!-- Log out-->
                            <li class="nav-item"><a href="@Url.Action("Login","Login")" class="nav-link logout"> <span class="d-none d-sm-inline-block">Logout</span><i class="fa fa-cog"></i></a></li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
        <!-- Counts Section -->
        <section class="dashboard-counts section-padding">
            <div class="container-fluid">
                <div class="row">
                    <!-- Count item widget-->
                    <div class="col-xl-2 col-md-4 col-6">
                        @Html.Action("_countProducts", "Page")
                    </div>
                    <!-- Count item widget-->
                    <div class="col-xl-2 col-md-4 col-6">
                       @Html.Action("_statusPromotin", "Page")
                    </div>
                    <!-- Count item widget-->
                    <div class="col-xl-2 col-md-4 col-6">
                        @Html.Action("_countCustomer","Page")
                    </div>
                    <!-- Count item widget-->
                    <div class="col-xl-2 col-md-4 col-6">
                        @Html.Action("_CountNews", "Page")
                    </div>
                    <!-- Count item widget-->
                    <div class="col-xl-2 col-md-4 col-6">
                      @Html.Action("_countCart", "Page")
                    </div>
                    <!-- Count item widget-->
                    <div class="col-xl-2 col-md-4 col-6">
                       @Html.Action("_countContacts","Page")
                    </div>
                </div>
            </div>
        </section>
        <!-- Header Section-->


        @RenderBody()
        <footer class="main-footer">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-6">
                        <p>Your company &copy; 2017-2019</p>
                    </div>
                    <div class="col-sm-6 text-right">
                        <p>Design by <a href="https://bootstrapious.com" class="external">CT239</a></p>
                        <!-- Please do not remove the backlink to us unless you support further theme's development at https://bootstrapious.com/donate. It is part of the license conditions and it helps me to run Bootstrapious. Thank you for understanding :)-->
                    </div>
                </div>
            </div>
        </footer>
    </div>
    <!-- JavaScript files-->
    <script src="~/Library/Admin/vendor/jquery/jquery.min.js"></script>


    <script src="~/Library/Admin/vendor/popper.js/popper.js"></script>
    <script src="~/Library/Admin/vendor/bootstrap/js/bootstrap.min.js"></script>

    <script src="~/Library/Admin/js/grasp_mobile_progress_circle-1.0.0.min.js"></script>

    <script src="~/Library/Admin/vendor/jquery.cookie/jquery.cookie.js"></script>

    <script src="~/Library/Admin/vendor/chart.js/Chart.js"></script>
    <script src="~/Library/Admin/vendor/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/Library/Admin/vendor/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>

    <script src="~/Library/Admin/js/charts-home.js"></script>

    <!-- Main File-->
    <script src="~/Library/Admin/js/front.js"></script>

    <link href="~/Library/Admin/DataTables/datatables.min.css" rel="stylesheet" />
    <script src="~/Library/Admin/DataTables/datatables.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#tablefillset').DataTable(
                {
                    "language": {
                        "infoEmpty": "Data null",
                        "emptyTable": "Null Data",
                        "processing": "Access...",
                        "search": "Search:",
                        "loadingRecords": "load...",
                        "zeroRecords": "Data cannot find",
                        "paginate": {
                            "first": "|<",
                            "last": ">|",
                            "next": ">>",
                            "previous": "<<"
                        }
                    },
                    "lengthMenu": [[5, 10, 15, 20, 25, 30, -1], [5, 10, 15, 20, 25, 30, "All"]]
                }

            );


        });
    </script>
    <link href="~/Library/Admin/css/jquery-ui.min.css" rel="stylesheet" />
    <script src="~/Library/Admin/js/jquery-ui.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>